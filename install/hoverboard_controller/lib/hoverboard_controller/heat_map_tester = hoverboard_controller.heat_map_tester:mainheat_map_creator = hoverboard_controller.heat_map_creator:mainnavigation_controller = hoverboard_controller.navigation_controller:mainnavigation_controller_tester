#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'hoverboard-controller','console_scripts','heat_map_tester = hoverboard_controller.heat_map_tester:mainheat_map_creator = hoverboard_controller.heat_map_creator:mainnavigation_controller = hoverboard_controller.navigation_controller:mainnavigation_controller_tester'
__requires__ = 'hoverboard-controller'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('hoverboard-controller', 'console_scripts', 'heat_map_tester = hoverboard_controller.heat_map_tester:mainheat_map_creator = hoverboard_controller.heat_map_creator:mainnavigation_controller = hoverboard_controller.navigation_controller:mainnavigation_controller_tester')()
    )
